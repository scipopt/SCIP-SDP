SCIPDIR = ../scipoptsuite-3.1.1/scip-3.1.1

USE_DSDP = true
DSDP_INCLUDE_DIR = ../DSDP5.8/include
DSDP_LIB_DIR =     ../DSDP5.8/lib

SDPA_INCLUDE_DIR = ../sdpa/include
SDPA_LIB_DIR =     ../sdpa/lib
FLIBS =  -L/usr/lib/gcc/x86_64-redhat-linux6E/4.4.7 -L/usr/lib/gcc/x86_64-redhat-linux6E/4.4.7/../../../../lib64 -L/lib/../lib64 -L/usr/lib/../lib64 -L/usr/lib/gcc/x86_64-redhat-linux6E/4.4.7/../../.. -lpthread -lgfortranbegin -lgfortran -lm -lgomp
MUMPS_DIR       =       ../mumps/build
SDPA_LDFLAGS    =       -L$(MUMPS_DIR)/lib -ldmumps -lmumps_common -lpord -L$(MUMPS_DIR)/libseq -lmpiseq $(FLIBS) # $(LAPACK_LIBS) $(BLAS_LIBS) $(FLIBS)
SDPA_FLAGS      =       -I$(MUMPS_DIR)/include
# -lmumps_common -lpord -L$(MUMPS_DIR)/libseq -lmpiseq $(LAPACK_LIBS) $(BLAS_LIBS) $(FLIBS)
